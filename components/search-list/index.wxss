.list-content{
  padding: 0 20rpx;
  position: absolute;
  top: 100rpx;
  left: 0;
  right: 0;
  bottom: 0;
  background: #eee;
}
.list-content .list-content-item{
  margin: 20rpx 0;
  display: flex;
  flex-direction: column;
  height: 100rpx;
  font-size: 28rpx;
  line-height: 50rpx;
}
.list-content .list-content-item .list-content-item1{
  flex: 1;
}
.list-content .list-content-item .list-content-item2{
  flex: 1;
  color: rgb(168, 168, 168);
}